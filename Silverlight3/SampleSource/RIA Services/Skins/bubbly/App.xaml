                                            <!-- Start: Previous button content -->
                                            <Button x:Name="PreviousButtonElement" Height="20" Width="34" Template="{StaticResource PreviousButtonTemplate}" Grid.Column="0" Grid.Row="0" Visibility="Collapsed"/>
