                                            <!-- Start: Month Content Grid -->
                                            <Grid x:Name="MonthGrid" Grid.Row="1" Grid.ColumnSpan="3" Visibility="Collapsed" Margin="8,0,8,8" RenderTransformOrigin="0.5,0.5">
