                                                            <Grid Margin="20.5,4,7.5,5">
                                                                <Path Stretch="Fill" RenderTransformOrigin="0.5,0.5" Data="M2.5400503,-2.4606738 L2.5400503,7.5014123 7.5606006,2.4808186 z" Stroke="#FF63BFF4">
                                                                    <Path.RenderTransform>
                                                                        <TransformGroup>
                                                                            <ScaleTransform ScaleX="1" ScaleY="1"/>
                                                                            <SkewTransform AngleX="0" AngleY="0"/>
                                                                            <RotateTransform Angle="-180"/>
                                                                            <TranslateTransform X="0" Y="0"/>
