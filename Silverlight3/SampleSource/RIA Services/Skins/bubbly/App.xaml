                                <Storyboard x:Name="Normal State">
                                    <ColorAnimation Storyboard.TargetName="fillColor" Storyboard.TargetProperty="Color" Duration="0" To="#FFFFFFFF"/>
                                </Storyboard>
                                <Storyboard x:Name="Normal AlternatingRow State">
                                    <ColorAnimation Storyboard.TargetName="fillColor" Storyboard.TargetProperty="Color" Duration="0" To="#FFFFFFFF"/>
                                </Storyboard>
                                <Storyboard x:Name="MouseOver State">
                                    <ColorAnimation Storyboard.TargetName="fillColor" Storyboard.TargetProperty="Color" Duration="0" To="#7FB8DDF3"/>
