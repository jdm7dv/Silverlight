                                            <Button x:Name="HeaderButtonElement" FontSize="11" Height="20" Width="100" HorizontalAlignment="Center" VerticalAlignment="Center"  Template="{StaticResource HeaderButtonTemplate}" Grid.Column="1" Grid.Row="0"/>
