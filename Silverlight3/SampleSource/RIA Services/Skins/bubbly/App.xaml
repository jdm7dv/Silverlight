                                                                <GradientStop Color="#51FFFFFF" Offset="1" />
                                                                <GradientStop Color="#66FFFFFF" Offset="0.325" />
                                                                <GradientStop Color="#1EFFFFFF" Offset="0.325" />
                                                            </LinearGradientBrush>

                                                            <!-- Visual states for the template -->
