                                <Storyboard x:Name="Normal State">
                                    <ColorAnimation Storyboard.TargetName="fillColor" Storyboard.TargetProperty="Color" Duration="0" To="#7FB8DDF3"/>
                                </Storyboard>
                                <Storyboard x:Name="MouseOver State">
                                    <ColorAnimation Storyboard.TargetName="fillColor" Storyboard.TargetProperty="Color" Duration="0" To="#7FB8DDF3"/>
                                </Storyboard>
                                <Storyboard x:Name="MouseOver CurrentRow Selected State">
                                    <ColorAnimation Storyboard.TargetName="fillColor" Storyboard.TargetProperty="Color" Duration="0" To="#7FB8DDF3"/>
                                </Storyboard>
                                <Storyboard x:Name="Normal CurrentRow State">
                                    <ColorAnimation Storyboard.TargetName="fillColor" Storyboard.TargetProperty="Color" Duration="0" To="#7FFBF724"/>
