                                                            <Color x:Key="Hover">#FFD7F1FF</Color>
                                                            <SolidColorBrush x:Key="DisabledColor" Color="#A5FFFFFF" />
                                                            <LinearGradientBrush x:Key="FocusedStrokeBrush" StartPoint="0.5,0" EndPoint="0.5,1">
                                                                <GradientStop Color="#B2FFFFFF" Offset="0" />
                                                                <GradientStop Color="#51FFFFFF" Offset="1" />
                                                                <GradientStop Color="#66FFFFFF" Offset="0.325" />
                                                                <GradientStop Color="#1EFFFFFF" Offset="0.325" />
                                                            </LinearGradientBrush>

                                                            <!-- Visual states for the template -->
                                                            <Storyboard x:Key="Normal State">
                                                                <DoubleAnimation Duration="0:0:0" Storyboard.TargetName="TextColor" Storyboard.TargetProperty="Opacity" To="1" />
                                                            </Storyboard>
                                                            <Storyboard x:Key="MouseOver State">
                                                                <ColorAnimation Duration="0:0:0" Storyboard.TargetName="TextColor" Storyboard.TargetProperty="Color" To="{StaticResource Hover}" />
                                                            </Storyboard>
                                                            <Storyboard x:Key="Pressed State">
                                                                <ColorAnimation Duration="0:0:0" Storyboard.TargetName="TextColor" Storyboard.TargetProperty="Color" To="{StaticResource Hover}" />
                                                            </Storyboard>
                                                            <Storyboard x:Key="Disabled State">
                                                                <DoubleAnimation Duration="0:0:0" Storyboard.TargetName="TextColor" Storyboard.TargetProperty="Opacity" To=".5" />
                                                            </Storyboard>
                                                        </Grid.Resources>
                                                        <Grid>
                                                            <Rectangle Fill="#FF70C5F4" Stretch="Fill" Margin="5,0,0,0" Opacity=".05"/>
                                                            <Grid Margin="7.5,5,20.5,4">
                                                                <Path Stretch="Fill" Data="M2.5400503,-2.4606738 L2.5400503,7.5014123 7.5606006,2.4808186 z" Stroke="#FF63BFF4">
