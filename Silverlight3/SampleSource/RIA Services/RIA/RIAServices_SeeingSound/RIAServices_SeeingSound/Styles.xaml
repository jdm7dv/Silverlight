  xmlns:dataControls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Data.DataForm.Toolkit" 
  xmlns:dataInput="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls.Data.Input">

  <!-- ******MAIN PAGE STYLES****** -->
  <!-- **************************** -->
  <!-- Primary Color Brushes -->
  <SolidColorBrush x:Key="NavigationBackgroundColorBrush" Color="#FFF0F0F0"/>
  <SolidColorBrush x:Key="NavigationBackground2ColorBrush" Color="#FFFFE480"/>
  <SolidColorBrush x:Key="NavigationBackgroundHighlightColorBrush" Color="#FFF6C000"/>
  <Color x:Key="NavigationBackgroundHighlightColor">#FFF6C000</Color>
  <SolidColorBrush x:Key="HighLightColorBrush" Color="#FFEAEAEA"/>
  <SolidColorBrush x:Key="BodyTextColorBrush" Color="#FF838383"/>
  <SolidColorBrush x:Key="AccentColorBrush" Color="#FFFF6EB9"/>
  <SolidColorBrush x:Key="NavigationForegroundColorBrush" Color="#BB000000"/>

  <!-- LayoutRoot Grid Style -->
  <Style x:Key="LayoutRootGridStyle" TargetType="Grid">
    <Setter Property="Background" Value="{StaticResource NavigationBackgroundColorBrush}"/>
  </Style>

  <!-- Content Border Style -->
  <Style x:Key="ContentBorderStyle" TargetType="Border">
    <Setter Property="Background" Value="{StaticResource NavigationBackground2ColorBrush}"/>
    <Setter Property="CornerRadius" Value="0"/>
    <Setter Property="Margin" Value="130,0,0,0"/>
    <Setter Property="BorderThickness" Value="12,0,0,0"/>
    <Setter Property="BorderBrush" Value="{StaticResource NavigationBackgroundHighlightColorBrush}"/>
  </Style>

  <!-- Content Frame Style -->
  <Style x:Key="ContentFrameStyle" TargetType="navigation:Frame">
    <Setter Property="Background" Value="White"/>
    <Setter Property="Margin" Value="165,25,-2,65"/>
    <Setter Property="Padding" Value="58,15,58,15"/>
    <Setter Property="VerticalContentAlignment" Value="Stretch"/>
    <Setter Property="HorizontalContentAlignment" Value="Stretch"/>
  </Style>

  <!-- Navigation Outer StackPanel Style -->
  <Style x:Key="NavigationOuterGridStyle" TargetType="Grid"/>

  <!-- Navigation Grid Style -->
  <Style x:Key="NavigationGridStyle" TargetType="Grid">
    <Setter Property="Background" Value="Transparent"/>
    <Setter Property="Margin" Value="-940,0,0,0"/>
    <Setter Property="Height" Value="120"/>
    <Setter Property="Width" Value="2000"/>
    <Setter Property="VerticalAlignment" Value="Top"/>
    <Setter Property="HorizontalAlignment" Value="Left"/>
    <Setter Property="RenderTransformOrigin" Value="0.5,0.5"/>
    <Setter Property="RenderTransform">
      <Setter.Value>
        <TransformGroup>
          <ScaleTransform/>
          <SkewTransform AngleX="0" AngleY="0"/>
          <RotateTransform Angle="270"/>
          <TranslateTransform/>
        </TransformGroup>
      </Setter.Value>
    </Setter>
  </Style>

